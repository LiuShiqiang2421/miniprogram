.notice {
	margin: 18rpx auto;
	width: 95%;
	height: 60rpx;
	background-color: #fcf8f2;
	border-radius: 5rpx;
}

.swiper-notice {
	width: 100%;
	height: 100%;
}

.swiper-item {
	display: flex;
}

.swiper-item image {
	height: 40rpx;
	width: 40rpx;
	margin-top: 10rpx;
	margin-left: 10rpx;
}

.swiper-item text {
	color: #ffbb40;
	font-size: 29rpx;
	margin-top: 10rpx;
	margin-left: 10rpx;
}

.scroll-view-container {
	display: flex;
	background-color: #f5f5f5;
}

.left-scroll-view {
	width: 250rpx;
	margin-top: 18rpx;
}

.left-scroll-view-item {
	height: 150rpx;
	font-size: 24rpx;
	color: #666;
	display: flex;
	flex-direction: column;
	justify-content: space-evenly;
}

.left-scroll-view-item image {
	width: 64rpx;
	height: 64rpx;
	margin-left: 55rpx;
}

.cate_name {
	text-align: center;
	margin: 0 auto;
}

.left-scroll-view-item.active {
	background-color: #e5e5e5;
	border-top-right-radius: 40rpx;
	border-bottom-right-radius: 40rpx;
	position: relative;
	color: #000;
	font-weight: 600;
}

.left-scroll-view-item.active::before {
	content: ' ';
	display: block;
	width: 6rpx;
	height: 60rpx;
	background-color: #ffbb40;
	position: absolute;
	top: 50%;
	left: 0;
	transform: translateY(-50%);
}

.right-scroll-view {
	border-radius: 25rpx;
	background-color: #fafbfc;
	margin: 16rpx;
}

.right-scroll-view-item {
	display: flex;
	margin: 0 16rpx 35rpx 18rpx;
}

.meal-category {
	font-size: 35rpx;
	margin: 28rpx 0 28rpx 18rpx;
	font-weight: 600;
}

.meal-pic {
	width: 220rpx;
	height: 180rpx;
	border-radius: 8rpx;
}

.meal-name-info-price {
	width: 274rpx;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	margin-left: 15rpx;
}

.meal-name {
	font-family: '黑体';
}

.meal-info {
	font-size: 26rpx;
	color: #999999;
}

.meal-sales {
	font-size: 26rpx;
	color: #999999;
}

.meal-price-add-to-cart {
	width: 100%;
	display: flex;
	justify-content: space-between;
	position: relative;
}

.meal-price {
	color: #f16443;
	line-height: 50rpx;
	font-size: 30rpx;
}

.hover-status {
	background: #000;
	opacity: 0.4;
}

.subtract-to-cart {
	width: 46rpx;
	height: 46rpx;
	position: absolute;
	right: 90rpx;
}

.subtract-status
{
	height: 39rpx;
	width: 39rpx;
	position: absolute;
	border-radius: 50%;
	right: 94rpx;
	top: 6rpx
}

.mealcount {
	width: 36rpx;
	color: #000;
	position: absolute;
	text-align: center;
	right: 50rpx;
	line-height: 50rpx;
	font-size: 28rpx;
}

.add-to-cart {
	width: 53rpx;
	height: 53rpx;
	position: relative;
}

.add-status {
	height: 39rpx;
	width: 39rpx;
	position: absolute;
	border-radius: 50%;
	right: 6rpx;
	top: 8rpx;
}

.doNotExistNow {
	font-size: 50rpx;
	color: #000;
	font-weight: 600;
	letter-spacing: 10rpx;
	margin: 0 auto;
	text-align: center;
	margin-top: 200rpx;
}

.modal-mask {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	opacity: 0.6;
	background: #000;
	z-index: 666;
}

.modal {
	display: flex;
	flex-direction: column;
	box-sizing: border-box;
	width: 90%;
	overflow: hidden;
	position: fixed;
	top: 10%;
	left: 0;
	right: 0;
	margin: 0 auto;
	z-index: 9999;
}

.modal-dialog {
	background: white;
	padding: 25rpx;
	border-radius: 10rpx;
}

.modal-meal-name {
	font-family: '黑体';
	font-size: 36rpx;
	font-weight: 500;
}

.modal-meal-pic {
	width: 100%;
	height: 500rpx;
	margin: 25rpx auto;
	border-radius: 10rpx;
}

.modal-meal-info {
	font-family: '黑体';
	font-size: 32rpx;
	padding-bottom: 20rpx;
	color: #666;
}

.modal-bottom {
	display: flex;
	justify-content: space-between;
}

.modal-meal-price {
	font-weight: 600;
	color: #f65249;
	font-size: 40rpx;
}

.modal-cancel {
	margin-top: 40rpx;
	margin-left: 310rpx;
}

.shoppingcart {
	position: fixed;
	right: 40rpx;
	bottom: 50rpx;
	height: 100rpx;
	width: 100rpx;
	border-radius: 50rpx;
	background-color: #fff;
}

.shoppingcart-img {
	width: 100rpx;
	height: 100rpx;
}

.count {
	color: red;
	position: fixed;
	font-weight: bold;
	right: 35rpx;
	bottom: 140rpx;
}

.shoppingcart-info {
	position: fixed;
	width: 710rpx;
	height: 110rpx;
	right: -710rpx;
	bottom: 50rpx;
	border-radius: 50rpx;
	background-color: #fff;
	display: flex;
	justify-content: space-between;
	box-shadow: 1rpx 1rpx 20rpx #aaa;
}

.all_money {
	width: 50%;
	height: 80rpx;
	line-height: 110rpx;
	text-align: center;
	font-size: 32rpx;
}

.settlement {
	color: #fff;
	font-weight: 800;
	width: 200rpx;
	background-color: #ffbb40;
	border-top-right-radius: 50rpx;
	border-bottom-right-radius: 50rpx;
	text-align: center;
	line-height: 110rpx;
}

/*购物车弹窗  */
.cart_popup {
	width: 100%;
	height: 285px;
	background: #fff;
	position: fixed;
	z-index: 999;
	left: 0;
	bottom: -285px;
	overflow: auto;
	border-top-left-radius: 20rpx;
	border-top-right-radius: 20rpx;
}

.shopcart_title {
	width: 100%;
	height: 70rpx;
	line-height: 80rpx;
	text-align: center;
	position: fixed;
	left: 0;
	background-color: #ffbb40;
	color: #fff;
	border-top-left-radius: 20rpx;
	border-top-right-radius: 20rpx;
}

.shopcart_close {
	position: absolute;
	left: 30rpx;
	font-size: 28rpx;
}

/*清空购物车  */
.shopcart_deleteAll {
	position: absolute;
	right: 30rpx;
	font-size: 28rpx;
}

.margin_r {
	margin-right: 30rpx;
}

/*购物车列表  */
.cart_cells {
	width: 100%;
	height: 98rpx;
	position: relative;
	border-bottom: 1px #dedede solid;
}

.cells_name {
	width: 40%;
	height: 50%;
	position: absolute;
	top: 0;
	left: 0;
	font-size: 14px;
	margin-left: 40rpx;
	padding-top: 10rpx;
}

.cells_price {
	width: 60%;
	height: 50%;
	position: absolute;
	top: 50rpx;
	left: 0;
	margin-left: 40rpx;
	font-size: 14px;
	padding-bottom: 10rpx;
	color: #FB4C22;
}

.price-box {
	position: absolute;
	display: flex;
	justify-content: space-between;
	width: 190rpx;
	margin: 20rpx auto;
	left: 55%;
}

.cartSub {
	width: 46rpx;
	height: 46rpx;
	margin-top: 2rpx;
	position: relative;
}

.cart-subtract-status
{
	height: 39rpx;
	width: 39rpx;
	position: absolute;
	z-index: 1;
	border-radius: 50%;
	top: 8rpx;
	left: 5rpx;
}

.value {
	font-size: 28rpx;
	margin: 0 20rpx;
	font-weight: bold;
}

.cartAdd {
	width: 53rpx;
	height: 53rpx;
}

.cart-add-status
{
	height: 39rpx;
	width: 39rpx;
	position: absolute;
	z-index: 1;
	border-radius: 50%;
	right: 6rpx;
	top: 8rpx;
}

/*删除购物车某项  */
.shopcart_delete {
	position: absolute;
	right: 40rpx;
	top: 25rpx;
}